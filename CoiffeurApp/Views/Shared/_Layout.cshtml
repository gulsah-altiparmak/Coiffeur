    <script>

        function deleteItem(url, id) {

            swal({
                title: 'Emin Misiniz?',
                text: "Yapılan İşlemi Geri Alamayacaksınız!",
                type: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Evet, Sil!',
                cancelButtonText: 'Çıkış'
            }).then((result) => {
                if (result.value) {

                    $.ajax({
                        type: 'post',
                        url: url + '/' + id,
                        success: function (data) {
                            if (data) {
                                //swal('Silindi!', 'Dosyan Silindi!', 'success');

                                swal(
                                    'Silindi!',
                                    'Dosya Silindi!',
                                    'success'
                                )
                                $("#tr" + id).fadeOut($("#tr" + id).remove());


                            }
                            else {
                                swal('Hata Oluştu!', 'İşlem sırasında hata oluştu!', 'warning');
                            }
                        }
                    });


                }
            })

        }


    </script>
    <script>
        function uploadImageFile(imageField) {

            var image_file = imageField.attributes["image-file"].value;
            var image_file_text = imageField.attributes["image-file-text"].value;
            var model = new FormData();
            model.append("File", $('#' + image_file)[0].files[0]);
            //  model.append("File", $('#' + image_file)[0].files[1]);

            model.append("Name", "Name");
            var url = "/User/UploadImageFile";
            $.ajax({
                url: url,
                type: "POST",
                data: model,

                contentType: false,
                //processData:
                //To prevent data in data option getting converted to string format, 'processData' option is set to false.
                processData: false,
                success: function (response) {
                    document.getElementById(image_file_text).value = response;
                }
            });


        }
    </script>
    <script>
        function LoadChart() {
            $.getJSON("/Home/GetChartBookAndService", function (data) {
                for (var i = 0; i < data.length; i++) {
                    var piece = data[i].Count + " " + data[i].label

                    $(piece).append("#m_flotcharts_10");
                }
                
            });
        }

    </script>
  
